<div class="jumbotron">
  <h1>Novedades constitucionales de hace <%= @user.periodicity %> días atrás</h1>
  <p>Participapp</p>
</div>

<% @contents.each do |content| %>
  <div class="well well-lg">
    <h2><%= content.title %></h2>
    <h4>Publicada el <%= content.created_at.strftime("%d:%m:%y") %></h4>
    <h5>Categoría: <%= Category.find_by(id: content.category_id).name %> </h5>

    <div>
      <%= "#{content.body}".html_safe %>
    </div>
  </div>
<% end %>
