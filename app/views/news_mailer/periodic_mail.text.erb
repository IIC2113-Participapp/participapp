ParticipApp
Novedades constitucionales de hace <%= pluralize(@user.periodicity, "día") %>:

<% @contents.each do |content| %>
    <%= content.title %>
    Publicada el <%= content.created_at.strftime("%d:%m:%y") %>
    Categoría: <%= Category.find_by(id: content.category_id).name %>
    <%= "#{content.body}".html_safe %>
    "\n\n"
<% end %>
